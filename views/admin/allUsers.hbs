

<div class="container" style="margin-top: 200px;">
<table class="table table-bordered table-dark" style="margin-top: 0;" >
  <thead class="thead-success">
    <tr>
      <th scope="col">No.</th>
      <th scope="col">First Name</th>
       <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col">Option</th>
    </tr>
  </thead>
  <tbody id="myTable">

      {{#each allUsers}}
    <tr class="bg-light">
      <td>{{@index}}</td>
      <td>{{this.fname}}</td>
       <td>{{this.lname}}</td>
      <td>{{this.email}}</td>
      <td>{{this.phone}}</td>
     <td><button onclick="deleteUser('{{this._id}}')"  class="btn btn-danger">Delete</button></td>
    </tr>
    {{/each}}
   
  
  </tbody>
</table>

 
