<body
    style="background-image: url(https://media.istockphoto.com/photos/women-weekend-picture-id1001589046?k=6&m=1001589046&s=612x612&w=0&h=4fhj8bBB4RaUj-SO5xi9xi3cWfqaUAPH77v_FAK_we8=); background-size:cover;background-repeat: no repeat;">


    

<form id="signup-form" action="/signup" method="POST">
    <div class="signup-form">
        <h1>signup-form</h1>
        {{#if response}}
         <h4 id="logged" style=" color: red;">Account already taked</h4>
        {{else}}
        
         {{/if}}
        
        <div>
    <label for="exampleInputEmail1" class="form-label">First Name</label>
    <input type="text" id="fname" name="fname" placeholder="First Name" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    <div id="emailHelp" class="form-text"></div>
  </div>
  <div >
    <label for="exampleInputEmail1" class="form-label">Last Name</label>
    <input type="text" id="lname" name="lname" placeholder="Last Name" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    <div id="emailHelp" class="form-text"></div>
  </div>

  <div >
    <label for="exampleInputEmail1" class="form-label">Email address</label>
    <input type="email" id="email" name="email" placeholder="Email address" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" >
    <div id="emailHelp" class="form-text"></div>
  </div>

  <div >
    <label for="exampleInputEmail1" class="form-label">Password</label>
    <input type="password" id="password" name="password" placeholder="Password" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" >
    <div id="emailHelp" class="form-text"></div>
  </div>
  <div class="mb-3">
    <label for="exampleInputPassword1" class="form-label">Phone</label>
    <input type="text" id="phone" name="phone" value="91" placeholder="Phone" class="form-control" id="exampleInputPassword1" >
    <input type="text" name="channel" value="sms" hidden>
  </div>
  <button type="submit" class="btn btn-primary ">Submit</button>
  </div>
</form>



